    <summary>Autofac extension allowing Autofac to be used with Microsoft Enterprise Library.</summary>
    <language>en-US</language>
    <title>Autofac Extras: Enterprise Library Configuration Support</title>
    <projectUrl>http://autofac.org</projectUrl>
    <iconUrl>http://code.google.com/p/autofac/logo</iconUrl>
    <dependencies>
      <dependency id="Autofac" version="$version$" />
      <dependency id="Autofac.Extras.CommonServiceLocator" version="$version$" />
      <dependency id="CommonServiceLocator" version="$version_CommonServiceLocator$" />
      <dependency id="EnterpriseLibrary.Common" version="$version_EnterpriseLibrary.Common$" />
    </dependencies>
  </metadata>
</package>